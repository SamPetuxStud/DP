#include <iostream>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;

int main()
{
	ios_base::sync_with_stdio(0);
	cin.tie(0); cout.tie(0);

	int n = 0;
	cin >> n;

	vector <int> fib(n);

	fib[0] = 1;
	fib[1] = 1;

	for (int i = 0; i < n; i++)
		fib[i] = fib[i - 1] + fib[i - 2];

	cout << fib[n - 1] << '\n';

	return 0;
}
